pipeline {
    agent any
environment {
    DOCKERHUB_CREDENTIALS = credentials('dockerhub')
     registry = "prathibhagond/backend-java"
     registryCredential = 'dockeruser'
     dockerImage = ''
}
stages {
 stage('creating the database container') {
        steps {
            sh 'docker container rm --force lmsdb'
            sh 'docker run -d -p 3307:3306 --network lms-network -e MYSQL_ROOT_PASSWORD=P@55word --name lmsdb mysql'
        }
}
}
}
